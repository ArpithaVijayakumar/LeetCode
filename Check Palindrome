#Time complexity is O(n)


def checkIfPalindromeOrNot(string):
    string = ''.join(string.split())
    d={}
    for x in string:
        d[x]=d.get(x,0) + 1
    count1 =0
    for x in d.values():
        if x != 2:
            return False
        if x ==0:
            if count1>0:
                return false 
            else:
                count+=1
    return True
    
print(checkIfPalindromeOrNot("abc abcd"))
